{
  "name": "@aigne/aigne-hub",
  "version": "0.8.5",
  "description": "AIGNE Hub SDK for integrating with Hub AI models",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://www.aigne.io/framework",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "bugs": {
    "url": "https://github.com/AIGNE-io/aigne-framework/issues"
  },
  "files": [
    "lib/cjs",
    "lib/dts",
    "lib/esm",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/dts/index.d.ts"
    }
  },
  "dependencies": {
    "@smithy/node-http-handler": "^4.1.0",
    "https-proxy-agent": "^7.0.6",
    "ufo": "^1.6.1",
    "zod": "^3.25.67",
    "@aigne/anthropic": "^0.11.16",
    "@aigne/bedrock": "^0.9.16",
    "@aigne/core": "^1.57.4",
    "@aigne/doubao": "^1.0.29",
    "@aigne/deepseek": "^0.7.35",
    "@aigne/gemini": "^0.11.5",
    "@aigne/ideogram": "^0.3.5",
    "@aigne/openai": "^0.13.6",
    "@aigne/ollama": "^0.7.35",
    "@aigne/open-router": "^0.7.35",
    "@aigne/poe": "^1.0.15",
    "@aigne/platform-helpers": "^0.6.2",
    "@aigne/xai": "^0.7.35",
    "@aigne/transport": "^0.14.15"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "detect-port": "^2.1.0",
    "hono": "^4.8.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "@aigne/test-utils": "^0.5.42"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "build": "tsc --build scripts/tsconfig.build.json",
    "clean": "rimraf lib test/coverage",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov --coverage-reporter=text",
    "postbuild": "node ../../scripts/post-build-lib.mjs"
  }
}