{
  "name": "@aigne/core",
  "version": "1.57.4",
  "description": "The functional core of agentic AI",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://www.aigne.io/framework",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "bugs": {
    "url": "https://github.com/AIGNE-io/aigne-framework/issues"
  },
  "files": [
    "lib/cjs",
    "lib/dts",
    "lib/esm",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/dts/index.d.ts"
    },
    "./*": {
      "import": "./lib/esm/*",
      "require": "./lib/cjs/*",
      "types": "./lib/dts/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/dts/index.d.ts"
      ],
      "agents/*": [
        "./lib/dts/agents/*"
      ],
      "aigne/*": [
        "./lib/dts/aigne/*"
      ],
      "loader/*": [
        "./lib/dts/loader/*"
      ],
      "memory/*": [
        "./lib/dts/memory/*"
      ],
      "prompt/*": [
        "./lib/dts/prompt/*"
      ],
      "utils/*": [
        "./lib/dts/utils/*"
      ]
    }
  },
  "dependencies": {
    "@aigne/json-schema-to-zod": "^1.3.3",
    "@inquirer/prompts": "^7.8.4",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@types/debug": "^4.1.12",
    "ajv": "^8.17.1",
    "camelize-ts": "^3.0.0",
    "content-type": "^1.0.5",
    "debug": "^4.4.1",
    "eventsource-parser": "^3.0.3",
    "fast-deep-equal": "^3.1.3",
    "fastq": "^1.19.1",
    "immer": "^10.1.1",
    "is-network-error": "^1.1.0",
    "jaison": "^2.0.2",
    "jsonata": "^2.0.6",
    "nunjucks": "^3.2.4",
    "p-retry": "^6.2.1",
    "raw-body": "^3.0.0",
    "strict-event-emitter": "^0.5.1",
    "ufo": "^1.6.1",
    "uuid": "^11.1.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.6",
    "@aigne/observability-api": "^0.10.1",
    "@aigne/platform-helpers": "^0.6.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/compression": "^1.8.1",
    "@types/content-type": "^1.1.9",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.12",
    "@types/nunjucks": "^3.2.6",
    "compression": "^1.8.0",
    "detect-port": "^2.1.0",
    "express": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "build": "tsc --build scripts/tsconfig.build.json",
    "clean": "rimraf lib test/coverage",
    "test": "bun --cwd test test",
    "test:coverage": "bun --cwd test test --coverage --coverage-reporter=lcov --coverage-reporter=text",
    "postbuild": "node ../../scripts/post-build-lib.mjs"
  }
}